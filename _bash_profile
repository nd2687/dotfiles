# ファイルの末尾に下記を追加する
PATH=/usr/local/bin:/usr/local/sbin:$PATH
export PATH

# rbenvのたmに追加した部分
if which rbenv > /dev/null; then eval "$(rbenv init -)"; fi
# if running bash
if [ -n "$BASH_VERSION" ]; then
    # include .bashrc if it exists
    if [ -f "$HOME/.bashrc" ]; then
    . "$HOME/.bashrc"
    fi
fi
eval "$(rbenv init -)"
export PATH="$HOME/.rbenv/bin:$PATH"
if which rbenv > /dev/null; then eval "$(rbenv init -)"; fi
